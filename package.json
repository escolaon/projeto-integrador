    "vue-use-active-scroll": "^1.1.1",
    "vue3-apexcharts": "^1.5.2",
    "vue3-datepicker": "^0.4.0",
    "yup": "^1.4.0",
    "zod": "^3.23.4"
  },
  "devDependencies": {
    "@nuxt/eslint": "^0.5.6",
